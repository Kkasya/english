{
    "env": {
        "browser": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "rules": {
        "no-plusplus": "off",
        "max-len": ["error", 150, { "ignoreUrls": true}],
        "no-tabs": "off",
        "no-console": "off",
        "linebreak-style": "off"
    }
}
